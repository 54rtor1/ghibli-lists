
<%= link_to "Create new movie list", new_list_path, class: "btn btn-flat" %>

<div class="container">
  <h1>my lists</h1>
  <div class="row">
  <% @lists.each do |list| %>
  <div class="col-sm-6">
    <div class="card">
      <div class="card-body">
        <h5 class="card-title"><%= link_to list_path(list), class: "btn btn-flat" do %>
             <%= list.name %>
               <% end %></h5>
        <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
      </div>
    </div>
  </div>
  <% end %>
  <%= link_to "Create new movie list", new_list_path, class: "btn btn-flat" %>
  </div>
</div><!--.container-->
